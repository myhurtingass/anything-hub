local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Choose your theme!", "DarkTheme")
local Tab = Window:NewTab("Theme Chooser")
local Section = Tab:NewSection("Before you get the gui, you must pick a theme first!")
Section:NewButton("LightTheme", "Sets the theme to LightTheme", function()
    print("Theme picked for Anything Hub")
end)
